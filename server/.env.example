# Database (Neon)
DATABASE_URL="postgres://user:password@endpoint.neon.tech/neondb?sslmode=require"

# Authentication (Lucia)
# No specific env needed for basic Lucia with Argon2, but strict session cookies need HTTPS in prod.
NODE_ENV="development"

# Google Cloud Storage (Service Account JSON or specific fields)
# GCS_CREDENTIALS="{...}" or path to file
GCS_PROJECT_ID="your-project-id"
GCS_BUCKET_NAME="your-bucket-name"
GCS_CLIENT_EMAIL="your-service-account-email"
GCS_PRIVATE_KEY="your-private-key"

# AI Provider Keys (Optional for server if stored in DB, but good for defaults)
GEMINI_API_KEY=""
OPENAI_API_KEY=""
